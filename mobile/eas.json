{
  "cli": {
    "version": ">= 9.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "KAKAO_JS_KEY": "env-secret",
        "FUNCTIONS_PROXY": "env-secret"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "KAKAO_JS_KEY": "env-secret",
        "FUNCTIONS_PROXY": "env-secret"
      }
    },
    "production": {
      "channel": "stable",
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m1-medium"
      },
      "android": {
        "buildType": "aab"
      },
      "env": {
        "KAKAO_JS_KEY": "env-secret",
        "FUNCTIONS_PROXY": "env-secret"
      }
    }
  },
  "submit": {
    "production": {
      "channel": "stable",
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "ABCDEFGHIJ"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      }
    }
  },
  "update": {
    "development": {
      "channel": "development"
    },
    "preview": {
      "channel": "preview"
    },
    "production": {
      "channel": "stable"
    }
  }
}
