{
  "name": "jaeman-vibe",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5175 --strictPort --open",
    "build": "tsc && vite build",
    "test": "vitest run -u || echo 'add your test runner here'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "cross-env APP_VERSION=$npm_package_version BUILD_TIME=$(date -u +%FT%TZ) node server.cjs",
    "rules:dev":  "powershell -NoProfile -Command \"Copy-Item -Force .\\firestore.rules.dev .\\firestore.rules; Copy-Item -Force .\\storage.rules.dev .\\storage.rules\"",
    "rules:prod": "powershell -NoProfile -Command \"Copy-Item -Force .\\firestore.rules.prod .\\firestore.rules; Copy-Item -Force .\\storage.rules.prod .\\storage.rules\"",
    "emu": "npx firebase-tools@latest emulators:start --config ./firebase.json --import ./.seed --export-on-exit",
    "web": "vite",
    "dev": "npm-run-all -p emu web",
    "test:emu": "npx firebase-tools@latest emulators:exec --project jaeman-vibe-platform \"npm test\"",
    "test:rules": "npx firebase-tools@latest emulators:exec --project jaeman-vibe-platform \"npm run test:rules\"",
    "test:rules:unit": "jest src/tests/rules.test.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@imgly/background-removal": "^1.7.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tensorflow-models/mobilenet": "^2.1.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "geofire-common": "^6.0.0",
    "lucide-react": "^0.539.0",
    "multer": "^1.4.5-lts.1",
    "onnxruntime-web": "^1.21.0",
    "openai": "^5.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@tailwindcss/cli": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "firebase-tools": "^14.12.1",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vitest": "^3.2.4"
  }
}
