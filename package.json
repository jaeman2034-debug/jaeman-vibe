{
  "name": "jaeman-vibe",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": "20.12.2"
  },
  "scripts": {
    "dev": "vite --host 127.0.0.1 --port 5183 --strictPort",
    "build": "vite build",
    "preview": "vite preview --host 127.0.0.1 --port 5183 --strictPort",
    "web": "vite --host 127.0.0.1 --port 5183 --strictPort",
    "dev5179": "vite --host 127.0.0.1 --port 5179 --strictPort",
    "blog-api": "node blog-api-server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "emu:up": "firebase emulators:start --config ./firebase.json --import ./.firebase-data --export-on-exit",
    "dev:all": "concurrently \"npm:emu:up\" \"npm:web\"",
    "emu:snapshot": "powershell -NoProfile -Command \"$stamp=(Get-Date -f yyyyMMdd-HHmm); firebase emulators:export ./.firebase-backups/$stamp\"",
    "deploy:rules": "firebase deploy --only firestore:rules,storage:rules --config firebase.prod.json -P jaeman-vibe-platform",
    "kill-ports": "kill-port 5177 5179 5183 8080 9099 9199 4310 4441 4511 9150",
    "reset": "npm run kill-ports && rimraf node_modules && npm ci",
    "seed": "node scripts/seed.mjs",
    "smoke": "cross-env EMU_HOST=127.0.0.1 EMU_AUTH_PORT=50199 EMU_FS_PORT=58081 EMU_STORAGE_PORT=59200 node scripts/smoke-test.mjs",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "market-service": "node market-service.js",
    "market-service:dev": "nodemon market-service.js",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "autoprefixer": "10.4.21",
    "concurrently": "8.2.2",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "firebase-tools": "13.20.2",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "lint-staged": "^16.1.6",
    "npm-run-all": "^4.1.5",
    "postcss": "8.5.6",
    "rimraf": "6.0.1",
    "tailwindcss": "^4.1.12",
    "ts-node": "^10.9.2",
    "vite": "7.1.3",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@resvg/resvg-js": "^2.6.2",
    "@sentry/react": "^10.11.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vite-plugin": "^4.3.0",
    "@tosspayments/payment-widget-sdk": "^0.12.0",
    "@types/qrcode": "^1.5.5",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "better-sqlite3": "^11.7.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cookie": "^1.0.2",
    "express": "^4.21.2",
    "firebase": "^12.2.1",
    "html2canvas": "^1.4.1",
    "idb": "^8.0.3",
    "idb-keyval": "^6.2.2",
    "json2csv": "^6.0.0-alpha.2",
    "jspdf": "^3.0.2",
    "jsqr": "^1.4.0",
    "lucide-react": "0.542.0",
    "maplibre-gl": "^5.7.1",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "2.6.0",
    "react-qr-reader": "^3.0.0-beta-1",
    "react-router-dom": "7.8.2",
    "recharts": "^3.2.0",
    "satori": "^0.18.2",
    "tailwind-merge": "3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.23.8"
  }
}
