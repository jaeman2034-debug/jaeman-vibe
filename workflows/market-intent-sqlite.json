      "id": "SqliteDelete",
      "name": "SQLite Delete",
      "type": "n8n-nodes-base.sqlite",
      "typeVersion": 1,
      "position": [850, 600],
      "credentials": { "sqlite": "local-sqlite" }
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={ \"ok\": true, \"intent\": $json.intent, \"data\": $json }"
      },
      "id": "Respond",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1100, 350]
    }
