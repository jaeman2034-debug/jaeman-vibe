          { "operation": "equal", "value": "market.list" },
          { "operation": "equal", "value": "market.updateStatus" },
          { "operation": "equal", "value": "market.delete" }
        ]
      },
      "id": "Switch",
      "name": "Switch Intent",
      "type": "n8n-nodes-base.switch",
      "typeVersion": 1,
      "position": [600, 300]
    },
    {
      "parameters": {
        "query": "INSERT INTO market (title, description, price) VALUES ({{ $json.payload.title }}, {{ $json.payload.description }}, {{ $json.payload.price }});"
      },
      "id": "SqliteCreate",
      "name": "SQLite Create",
      "type": "n8n-nodes-base.sqlite",
      "typeVersion": 1,
      "position": [850, 200],
      "credentials": { "sqlite": "local-sqlite" }
    },
    {
      "parameters": {
