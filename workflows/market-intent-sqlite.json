      "typeVersion": 1,
      "position": [850, 300],
      "credentials": { "sqlite": "local-sqlite" }
    },
    {
      "parameters": {
        "query": "SELECT * FROM market;"
      },
      "id": "SqliteList",
      "name": "SQLite List",
      "type": "n8n-nodes-base.sqlite",
      "typeVersion": 1,
      "position": [850, 400],
      "credentials": { "sqlite": "local-sqlite" }
    },
    {
      "parameters": {
        "query": "UPDATE market SET status={{ $json.payload.status }} WHERE id={{ $json.payload.id }};"
      },
      "id": "SqliteUpdate",
      "name": "SQLite Update",
      "type": "n8n-nodes-base.sqlite",
      "typeVersion": 1,
      "position": [850, 500],
      "credentials": { "sqlite": "local-sqlite" }
    },
    {
